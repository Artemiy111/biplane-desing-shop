{
  "name": "biplane-shop",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint '**/*.{css,vue}'",
    "lint:style:fix": "stylelint '**/*.{css,vue}' --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:dropmigtaion": "drizzle-kit dropmigration",
    "db:seed": "bun ./server/db/seed.ts",
    "db:drop": "bun ./server/db/drop.ts",
    "db:create": "docker exec -it biplane-shop-postgres-1 psql -U postgres -c \"CREATE DATABASE biplane_shop;\"",
    "db:destroy": "docker exec -it biplane-shop-postgres-1 psql -U postgres -c \"DROP DATABASE IF EXISTS biplane_shop;\"",
    "db:init": "bun run db:destroy && bun run db:create && bun run db:push && bun run db:seed"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.43",
