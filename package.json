{
  "name": "biplane-shop",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint '**/*.{css,vue}'",
    "lint:style:fix": "stylelint '**/*.{css,vue}' --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:dropmigtaion": "drizzle-kit dropmigration",
    "db:seed": "bun ./server/db/seed.ts",
    "db:drop": "bun ./server/db/drop.ts",
    "db:create": "docker exec -it biplane-shop-postgres-1 psql -U postgres -c \"CREATE DATABASE biplane_shop;\"",
    "db:destroy": "docker exec -it biplane-shop-postgres-1 psql -U postgres -c \"DROP DATABASE IF EXISTS biplane_shop;\"",
    "db:init": "bun run db:destroy && bun run db:create && bun run db:push && bun run db:seed"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.38",
    "@nuxt/eslint": "1.3.0",
    "@nuxt/fonts": "0.11.1",
    "@nuxt/icon": "1.12.0",
    "@nuxt/image": "1.10.0",
    "@nuxt/scripts": "0.11.6",
    "@nuxt/ui": "3.0.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@pinia/colada": "^0.15.0",
    "@pinia/colada-nuxt": "0.1.2",
    "@pinia/nuxt": "0.11.0",
    "@trpc/client": "^11.1.0",
    "@trpc/server": "^11.1.0",
    "@unhead/vue": "^2.0.3",
    "@vueuse/core": "^13.1.0",
    "@vueuse/nuxt": "^13.1.0",
    "better-auth": "^1.2.7",
    "clsx": "^2.1.1",
    "drizzle-orm": "^1.0.0-beta.1-19a7308",
    "eslint": "^9.0.0",
    "lucide-vue-next": "^0.503.0",
    "nodemailer": "^6.10.1",
    "nuxt": "^3.16.2",
    "pg": "^8.14.1",
    "pinia": "^3.0.2",
    "tailwind-merge": "^3.2.0",
    "trpc-nuxt": "^1.0.5",
    "typescript": "^5.6.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^4.0.0-beta.20250420T053007"
  },
  "devDependencies": {
    "@types/bun": "^1.2.10",
    "@types/nodemailer": "^6.4.17",
    "drizzle-kit": "^1.0.0-beta.1-19a7308"
  }
}
