    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@pinia/colada": "^0.15.3",
    "@pinia/colada-nuxt": "0.1.3",
    "@pinia/nuxt": "0.11.0",
    "@tresjs/cientos": "^4.3.0",
    "@tresjs/nuxt": "3.0.8",
    "@trpc/client": "^11.1.2",
    "@trpc/server": "^11.1.2",
    "@types/amqplib": "^0.10.7",
    "@unhead/vue": "^2.0.8",
